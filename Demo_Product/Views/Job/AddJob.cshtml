@model Job
@{
    ViewData["Title"] = "AddJob";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<h1>Meslek Ekleme Sayfası</h1>

<form method="post">
    <br />
    @Html.Label("Meslek İsmi")
    @Html.TextBoxFor(x=>x.Name,new { @class="form-control"})
    <br />

    <button class="btn btn-outline-info">Ekle</button>
</form>


